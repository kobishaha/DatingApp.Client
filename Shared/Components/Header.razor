<MudAppBar Color="Color.Primary" Elevation="4">
    <MudText Typo="Typo.h5">Dating App</MudText>
    <MudSpacer />
    <MudButton Color="Color.Secondary" OnClick="@(() => Navigation.NavigateTo("/login"))">Login</MudButton>
    <MudButton Color="Color.Secondary" OnClick="@(() => Navigation.NavigateTo("/register"))">Register</MudButton>
</MudAppBar>

@code {
    [Inject]
    public required NavigationManager Navigation { get; set; }
}
